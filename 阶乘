function factorial(num) {
    if (num < 0) {
    return '输入的数不能为负数';
    } else if (num === 0) {
        return 1;
    } else {
        var result =1;
         for (var i =1; i <= num; i++) {
         result *= i;
        }
        return result;
    }
}

console.log(factorial(5)); // 输出：120
console.log(factorial(0)); // 输出：1